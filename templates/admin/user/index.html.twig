{% extends 'admin/adminbase.html.twig' %}
{% block body %}
    <a href="{{ path('app_logout') }}">Выйти из учётной записи</a>

    <h1>
        {{ title }}
    </h1>

    <a href="{{ path('admin_user_create') }}">Добавить пользователя</a><br>
    
    <h2>Список пользователей</h2>
    <ul>
        {% for user in users %}
            <li>{{ user.email }} | {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}